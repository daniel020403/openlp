<?xml version='1.0' encoding='UTF-8'?>
<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.4.6" modifiedIn="OpenLP 2.4.6" modifiedDate="2023-09-10T03:56:05">
  <properties>
    <titles>
      <title>Firmly Committed</title>
    </titles>
    <verseOrder>v1 c1 c1 c1 v2 v2 c1 c1 c1 c1 c1 c1 c1 c1 c1 c1 c2 c1</verseOrder>
    <authors>
      <author>Author Unknown</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="v1a">
      <lines>I have surrendered</lines>
    </verse>
    <verse name="v1b">
      <lines>To Jesus my Lord,</lines>
    </verse>
    <verse name="v1c">
      <lines>I yield to His will,</lines>
    </verse>
    <verse name="v1d">
      <lines>I lay down my sword</lines>
    </verse>
    <verse name="v1e">
      <lines>To pick up His banner</lines>
    </verse>
    <verse name="v1f">
      <lines>And march bold and strong</lines>
    </verse>
    <verse name="v1g">
      <lines>There is no doubt</lines>
    </verse>
    <verse name="v1h">
      <lines>Whose side I am on,</lines>
    </verse>
    <verse name="c1a">
      <lines>I am firmly committed</lines>
    </verse>
    <verse name="c1b">
      <lines>To the cause of Christ</lines>
    </verse>
    <verse name="c1c">
      <lines>Determined to serve Him for the</lines>
    </verse>
    <verse name="c1d">
      <lines>Rest of my life</lines>
    </verse>
    <verse name="c1e">
      <lines>Resolved to give all</lines>
    </verse>
    <verse name="c1f">
      <lines>No matter the price</lines>
    </verse>
    <verse name="v2a">
      <lines/>
    </verse>
    <verse name="v2b">
      <lines>I've come through the valley</lines>
    </verse>
    <verse name="v2c">
      <lines>There's no turning round</lines>
    </verse>
    <verse name="v2d">
      <lines>I will press toward</lines>
    </verse>
    <verse name="v2e">
      <lines>The Lord's higher ground</lines>
    </verse>
    <verse name="v2f">
      <lines>The way set before me,</lines>
    </verse>
    <verse name="v2g">
      <lines>My only concern,</lines>
    </verse>
    <verse name="v2h">
      <lines>For I have passed</lines>
    </verse>
    <verse name="v2i">
      <lines>The point of return</lines>
    </verse>
    <verse name="c2a">
      <lines>All to Jesus</lines>
    </verse>
    <verse name="c2b">
      <lines>I surrender</lines>
    </verse>
    <verse name="c2c">
      <lines>To The Cause of</lines>
    </verse>
    <verse name="c2d">
      <lines>Christ!</lines>
    </verse>
    <verse name="c2e">
      <lines>OF CHRIST!</lines>
    </verse>
  </lyrics>
