<?xml version='1.0' encoding='UTF-8'?>
<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 3.0.2" modifiedIn="OpenLP 3.0.2" modifiedDate="2023-10-29T06:45:30">
  <properties>
    <titles>
      <title>Lead Me Lord</title>
    </titles>
    <copyright>Public Domain</copyright>
    <authors>
      <author>Samuel Sebastian Wesley</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="c1a">
      <lines>Lead me, Lord,<br/>Lead me in Thy righteousness;<br/>Make Thy way plain before my face.</lines>
    </verse>
    <verse name="c1b">
      <lines>For it is Thou, Lord,<br/>Thou, Lord, only,<br/>That makest me dwell in safety.</lines>
    </verse>
  </lyrics>
</song>
