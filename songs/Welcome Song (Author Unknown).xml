  <properties>
    <titles>
      <title>Welcome Song</title>
    </titles>
    <authors>
      <author>Author Unknown</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="v1a">
      <lines break="optional">Welcome,<br/>welcome all of you<br/></lines>
      <lines>I'm glad you're with us</lines>
    </verse>
    <verse name="v1b">
      <lines break="optional">Shake hands<br/>never to be blue<br/></lines>
      <lines>Welcome to you.</lines>
    </verse>
    <verse name="v2">
      <lines break="optional">There's a welcome here<br/>There's a welcome here<br/></lines>
      <lines break="optional">There's a Christian<br/>welcome here<br/></lines>
      <lines>Hallelujah.</lines>
    </verse>
    <verse name="v3">
      <lines break="optional">There's a welcome here<br/>There's a welcome here<br/></lines>
      <lines>There's a Christian<br/>welcome here.</lines>
    </verse>
  </lyrics>
